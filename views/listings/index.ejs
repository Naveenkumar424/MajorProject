<% layout('/layouts/boilerplate')-%>
<body>
    <h1>All listings</h1>
    <form method="get" action="/listings/new">
        <button>Create new listing</button>
    </form>
    <ul>
        <%for(let list of listings){%>
            <li><a href="/listings/<%=list._id%>"><%=list.title%></a></li>
        <%}%>
    </ul>
</body>
